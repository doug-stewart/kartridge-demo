{
    "plugins": ["prettier-plugin-astro"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ],
    "bracketSameLine": false,
    "printWidth": 100,
    "proseWrap": "always",
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": false
}
